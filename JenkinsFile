// This is a JenkinsFile that will be used to build the project

pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World, welcome to Jenkins'
            }
        }
        stage('Git') {
            steps {
                git branch: 'main', credentialsId: 'Git token', url: 'https://github.com/emDevanshu/Expense_Tracker.git'
            }
        }
    }
}